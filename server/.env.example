# Environment Configuration for OneSecClone Analytics Server
# Copy this file to .env and update the values

# Server Configuration
PORT=8080
NODE_ENV=production

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=onesec_analytics
DB_USER=onesec_user
DB_PASSWORD=your_secure_password_here

# Security (optional - for JWT tokens if you add authentication later)
JWT_SECRET=your_jwt_secret_here

# Logging
LOG_LEVEL=info
